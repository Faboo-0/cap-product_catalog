CREATE VIEW localized_de_com_fabboo_sales_SelProducts3 AS SELECT
  ProductReview_1.Rating,
  Products_0.Name,
  sum(Products_0.Price) AS TotalPrice
FROM (localized_de_com_fabboo_materials_Products AS Products_0 LEFT JOIN localized_de_com_fabboo_materials_ProductReview AS ProductReview_1 ON Products_0.Name = ProductReview_1.Name)
GROUP BY ProductReview_1.Rating, Products_0.Name
ORDER BY Rating;

CREATE VIEW localized_fr_com_fabboo_sales_SelProducts3 AS SELECT
  ProductReview_1.Rating,
  Products_0.Name,
  sum(Products_0.Price) AS TotalPrice
FROM (localized_fr_com_fabboo_materials_Products AS Products_0 LEFT JOIN localized_fr_com_fabboo_materials_ProductReview AS ProductReview_1 ON Products_0.Name = ProductReview_1.Name)
GROUP BY ProductReview_1.Rating, Products_0.Name
ORDER BY Rating;

CREATE VIEW localized_de_com_fabboo_reports_Products AS SELECT
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnit_ID,
  Products_0.Category_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination,
  ToAverageRating_1.AverageRating AS Rating,
  CASE WHEN Products_0.Quantity >= 8 THEN 3 WHEN Products_0.Quantity > 0 THEN 2 ELSE 1 END AS StockAvailability
FROM (localized_de_com_fabboo_materials_Products AS Products_0 LEFT JOIN com_fabboo_reports_AverageRating AS ToAverageRating_1 ON ToAverageRating_1.ProductId = Products_0.ID);

CREATE VIEW localized_fr_com_fabboo_reports_Products AS SELECT
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnit_ID,
  Products_0.Category_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination,
  ToAverageRating_1.AverageRating AS Rating,
  CASE WHEN Products_0.Quantity >= 8 THEN 3 WHEN Products_0.Quantity > 0 THEN 2 ELSE 1 END AS StockAvailability
FROM (localized_fr_com_fabboo_materials_Products AS Products_0 LEFT JOIN com_fabboo_reports_AverageRating AS ToAverageRating_1 ON ToAverageRating_1.ProductId = Products_0.ID);

CREATE VIEW localized_de_ProductService_Reviews AS SELECT
  ProductReview_0.ID,
  ProductReview_0.Name,
  ProductReview_0.Rating,
  ProductReview_0.Comment,
  ProductReview_0.Product_ID AS ToProduct_ID
FROM localized_de_com_fabboo_materials_ProductReview AS ProductReview_0;

CREATE VIEW localized_fr_ProductService_Reviews AS SELECT
  ProductReview_0.ID,
  ProductReview_0.Name,
  ProductReview_0.Rating,
  ProductReview_0.Comment,
  ProductReview_0.Product_ID AS ToProduct_ID
FROM localized_fr_com_fabboo_materials_ProductReview AS ProductReview_0;

CREATE VIEW localized_de_ProductService_Products AS SELECT
  Products_0.ID,
  Products_0.Name AS ProductName,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.UnitOfMeasure_ID AS ToUnitOfMeasure_ID,
  Products_0.Currency_ID AS ToCurrency_ID,
  Products_0.Category_ID AS ToCategory_ID,
  Category_1.Name AS Category,
  Products_0.DimensionUnit_ID AS ToDimensionUnit_ID,
  Products_0.Supplier_ID,
  Products_0.Rating,
  Products_0.StockAvailability
FROM (localized_de_com_fabboo_reports_Products AS Products_0 LEFT JOIN localized_de_com_fabboo_materials_Categories AS Category_1 ON Products_0.Category_ID = Category_1.ID);

CREATE VIEW localized_fr_ProductService_Products AS SELECT
  Products_0.ID,
  Products_0.Name AS ProductName,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.UnitOfMeasure_ID AS ToUnitOfMeasure_ID,
  Products_0.Currency_ID AS ToCurrency_ID,
  Products_0.Category_ID AS ToCategory_ID,
  Category_1.Name AS Category,
  Products_0.DimensionUnit_ID AS ToDimensionUnit_ID,
  Products_0.Supplier_ID,
  Products_0.Rating,
  Products_0.StockAvailability
FROM (localized_fr_com_fabboo_reports_Products AS Products_0 LEFT JOIN localized_fr_com_fabboo_materials_Categories AS Category_1 ON Products_0.Category_ID = Category_1.ID);

