
CREATE TABLE com_fabboo_materials_Products (
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(5000) NOT NULL,
  Description NVARCHAR(5000),
  ImageUrl NVARCHAR(5000),
  ReleaseDate TIMESTAMP_TEXT DEFAULT CURRENT_TIMESTAMP,
  DiscontinuedDate TIMESTAMP_TEXT,
  Price DECIMAL(16, 2),
  Height DECIMAL(16, 2),
  Width DECIMAL(16, 2),
  Depth DECIMAL(16, 2),
  Quantity DECIMAL(16, 2),
  Supplier_ID NVARCHAR(36),
  UnitOfMeasure_ID NVARCHAR(2),
  Currency_ID NVARCHAR(3),
  DimensionUnit_ID NVARCHAR(2),
  Category_ID NVARCHAR(1),
  PriceCondition NVARCHAR(2),
  PriceDetermination NVARCHAR(3),
  PRIMARY KEY(ID)
);

CREATE TABLE com_fabboo_materials_Categories (
  ID NVARCHAR(1) NOT NULL,
  Name NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_fabboo_materials_StockAvailability (
  ID INTEGER NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_fabboo_materials_Currencies (
  ID NVARCHAR(3) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_fabboo_materials_UnitOfMeasures (
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_fabboo_materials_DimensionUnit (
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_fabboo_materials_ProductReview (
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(5000),
  Rating INTEGER,
  Comment NVARCHAR(5000),
  Product_ID NVARCHAR(36),
  PRIMARY KEY(ID)
);

CREATE TABLE com_fabboo_sales_Order (
  ID NVARCHAR(36) NOT NULL,
  Date DATE_TEXT,
  Customer NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_fabboo_sales_OrderItem (
  ID NVARCHAR(36) NOT NULL,
  Order_ID NVARCHAR(36),
  Product_ID NVARCHAR(36),
  PRIMARY KEY(ID)
);

CREATE TABLE com_fabboo_sales_Suppliers (
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(5000) NOT NULL,
  Adress_Street NVARCHAR(5000),
  Adress_City NVARCHAR(5000),
  Adress_State NVARCHAR(2),
  Adress_PostalCode NVARCHAR(5),
  Adress_Country NVARCHAR(3),
  Email NVARCHAR(5000),
  Phone NVARCHAR(5000),
  Fax NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE com_fabboo_sales_Months (
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  ShortDescription NVARCHAR(3),
  PRIMARY KEY(ID)
);

CREATE TABLE com_fabboo_sales_SalesData (
  ID NVARCHAR(36) NOT NULL,
  DeliveryDate TIMESTAMP_TEXT,
  Revenue DECIMAL(16, 2),
  Product_ID NVARCHAR(36),
  Currency_ID NVARCHAR(3),
  DeliveryMonth_ID NVARCHAR(2),
  PRIMARY KEY(ID)
);

CREATE TABLE com_training_Course (
  ID NVARCHAR(36) NOT NULL,
  PRIMARY KEY(ID)
);

CREATE TABLE com_training_Student (
  ID NVARCHAR(36) NOT NULL,
  PRIMARY KEY(ID)
);

CREATE TABLE com_training_StudentCourse (
  ID NVARCHAR(36) NOT NULL,
  Course_ID NVARCHAR(36),
  Student_ID NVARCHAR(36),
  PRIMARY KEY(ID)
);

CREATE TABLE com_training_Orders (
  ClientEmail NVARCHAR(65) NOT NULL,
  FirstName NVARCHAR(30),
  LastName NVARCHAR(30),
  CreatedOn DATE_TEXT,
  Reviewed BOOLEAN,
  Approved BOOLEAN,
  PRIMARY KEY(ClientEmail)
);

CREATE TABLE com_fabboo_materials_Products_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(5000) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE com_fabboo_materials_Categories_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(1) NOT NULL,
  Name NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE com_fabboo_materials_StockAvailability_texts (
  locale NVARCHAR(14) NOT NULL,
  ID INTEGER NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE com_fabboo_materials_Currencies_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(3) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE com_fabboo_materials_UnitOfMeasures_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE com_fabboo_materials_DimensionUnit_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE com_fabboo_sales_Months_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  ShortDescription NVARCHAR(3),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE com_fabboo_sales_Suppliers_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(5000) NOT NULL,
  PRIMARY KEY(locale, ID)
);

CREATE VIEW ProductService_Supplier AS SELECT
  Suppliers_0.ID,
  Suppliers_0.Name,
  Suppliers_0.Email,
  Suppliers_0.Phone,
  Suppliers_0.Fax
FROM com_fabboo_sales_Suppliers AS Suppliers_0;

CREATE VIEW ProductService_Reviews AS SELECT
  ProductReview_0.ID,
  ProductReview_0.Name,
  ProductReview_0.Rating,
  ProductReview_0.Comment,
  ProductReview_0.Product_ID AS ToProduct_ID
FROM com_fabboo_materials_ProductReview AS ProductReview_0;

CREATE VIEW ProductService_SalesData AS SELECT
  SalesData_0.ID,
  SalesData_0.DeliveryDate,
  SalesData_0.Revenue,
  SalesData_0.Currency_ID AS CurrencyKey,
  DeliveryMonth_1.ID AS DeliveryMonthId,
  DeliveryMonth_1.Description AS DeliveryMonth,
  SalesData_0.Product_ID AS ToProduct_ID
FROM (com_fabboo_sales_SalesData AS SalesData_0 LEFT JOIN com_fabboo_sales_Months AS DeliveryMonth_1 ON SalesData_0.DeliveryMonth_ID = DeliveryMonth_1.ID);

CREATE VIEW ProductService_StockAvailability AS SELECT
  StockAvailability_0.ID,
  StockAvailability_0.Description
FROM com_fabboo_materials_StockAvailability AS StockAvailability_0;

CREATE VIEW ProductService_VH_Categories AS SELECT
  Categories_0.ID AS Code,
  Categories_0.Name AS Text
FROM com_fabboo_materials_Categories AS Categories_0;

CREATE VIEW ProductService_VH_Currencies AS SELECT
  Currencies_0.ID AS Code,
  Currencies_0.Description AS Text
FROM com_fabboo_materials_Currencies AS Currencies_0;

CREATE VIEW ProductService_VH_UnitOfMeasure AS SELECT
  UnitOfMeasures_0.ID AS Code,
  UnitOfMeasures_0.Description AS Text
FROM com_fabboo_materials_UnitOfMeasures AS UnitOfMeasures_0;

CREATE VIEW ProductService_VH_DimenionUnits AS SELECT
  DimensionUnit_0.ID AS Code,
  DimensionUnit_0.Description AS Text
FROM com_fabboo_materials_DimensionUnit AS DimensionUnit_0;

CREATE VIEW MyService_SuppliersProduct AS SELECT
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnit_ID,
  Products_0.Category_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination,
  Supplier_1.Adress_Street,
  Supplier_1.Adress_City,
  Supplier_1.Adress_State,
  Supplier_1.Adress_PostalCode,
  Supplier_1.Adress_Country
FROM (com_fabboo_materials_Products AS Products_0 LEFT JOIN com_fabboo_sales_Suppliers AS Supplier_1 ON Products_0.Supplier_ID = Supplier_1.ID)
WHERE (Supplier_1.Adress_PostalCode = 98074) AND (Products_0.Name = 'Pink Lemonade');

CREATE VIEW MyService_SuppliersToSales AS SELECT
  Supplier_1.Email,
  Category_2.Name,
  Currency_4.ID,
  Currency_4.Description
FROM ((((com_fabboo_materials_Products AS Products_0 LEFT JOIN com_fabboo_sales_Suppliers AS Supplier_1 ON Products_0.Supplier_ID = Supplier_1.ID) LEFT JOIN com_fabboo_materials_Categories AS Category_2 ON Products_0.Category_ID = Category_2.ID) LEFT JOIN com_fabboo_sales_SalesData AS SalesData_3 ON (SalesData_3.Product_ID = Products_0.ID)) LEFT JOIN com_fabboo_materials_Currencies AS Currency_4 ON SalesData_3.Currency_ID = Currency_4.ID);

CREATE VIEW MyService_FiltroEnfix AS SELECT
  Supplier_1.Phone
FROM (com_fabboo_materials_Products AS Products_0 LEFT JOIN com_fabboo_sales_Suppliers AS Supplier_1 ON (Products_0.Supplier_ID = Supplier_1.ID) AND (Supplier_1.Name = 'Exotic Liquids'))
WHERE Products_0.Name = 'Pink Lemonade';

CREATE VIEW MyService_FiltroJoin AS SELECT
  sup_1.Phone
FROM ((com_fabboo_materials_Products AS Products_0 LEFT JOIN com_fabboo_sales_Suppliers AS sup_1 ON (sup_1.ID = Products_0.Supplier_ID) AND sup_1.Name = 'Exotic Liquids') LEFT JOIN com_fabboo_materials_Products AS Product_2 ON (Product_2.Supplier_ID = sup_1.ID))
WHERE Product_2.Name = 'Pink Lemonade';

CREATE VIEW Reports_EntityCasting AS SELECT
  CAST(Products_0.Price AS INTEGER) AS Price,
  Products_0.Price AS Price02
FROM com_fabboo_materials_Products AS Products_0;

CREATE VIEW Reports_EntityExists AS SELECT
  Products_0.Name
FROM com_fabboo_materials_Products AS Products_0
WHERE EXISTS (SELECT
    1 AS dummy
  FROM com_fabboo_sales_Suppliers AS _Supplier_exists_1
  WHERE _Supplier_exists_1.ID = Products_0.Supplier_ID AND _Supplier_exists_1.Name = 'Exotic Liquids');

CREATE VIEW com_fabboo_materials_SelProducts AS SELECT
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnit_ID,
  Products_0.Category_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination
FROM com_fabboo_materials_Products AS Products_0;

CREATE VIEW com_fabboo_materials_ProjProducts AS SELECT
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnit_ID,
  Products_0.Category_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination
FROM com_fabboo_materials_Products AS Products_0;

CREATE VIEW com_fabboo_materials_ProjProducts2 AS SELECT
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnit_ID,
  Products_0.Category_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination
FROM com_fabboo_materials_Products AS Products_0;

CREATE VIEW com_fabboo_materials_ProjProducts3 AS SELECT
  Products_0.ReleaseDate,
  Products_0.Name
FROM com_fabboo_materials_Products AS Products_0;

CREATE VIEW com_fabboo_sales_SelProducts1 AS SELECT
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnit_ID,
  Products_0.Category_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination
FROM com_fabboo_materials_Products AS Products_0;

CREATE VIEW com_fabboo_sales_SelPoducts2 AS SELECT
  Products_0.Name,
  Products_0.Price,
  Products_0.Quantity
FROM com_fabboo_materials_Products AS Products_0;

CREATE VIEW com_fabboo_sales_SelProducts3 AS SELECT
  ProductReview_1.Rating,
  Products_0.Name,
  sum(Products_0.Price) AS TotalPrice
FROM (com_fabboo_materials_Products AS Products_0 LEFT JOIN com_fabboo_materials_ProductReview AS ProductReview_1 ON Products_0.Name = ProductReview_1.Name)
GROUP BY ProductReview_1.Rating, Products_0.Name
ORDER BY Rating;

CREATE VIEW com_fabboo_reports_AverageRating AS SELECT
  ProductReview_0.Product_ID AS ProductId,
  avg(ProductReview_0.Rating) AS AverageRating
FROM com_fabboo_materials_ProductReview AS ProductReview_0
GROUP BY ProductReview_0.Product_ID;

CREATE VIEW MyService_SuppliersProduct_texts AS SELECT
  texts_0.locale,
  texts_0.ID,
  texts_0.Name,
  texts_0.Description
FROM com_fabboo_materials_Products_texts AS texts_0;

CREATE VIEW localized_com_fabboo_materials_Products AS SELECT
  L_0.ID,
  coalesce(localized_1.Name, L_0.Name) AS Name,
  coalesce(localized_1.Description, L_0.Description) AS Description,
  L_0.ImageUrl,
  L_0.ReleaseDate,
  L_0.DiscontinuedDate,
  L_0.Price,
  L_0.Height,
  L_0.Width,
  L_0.Depth,
  L_0.Quantity,
  L_0.Supplier_ID,
  L_0.UnitOfMeasure_ID,
  L_0.Currency_ID,
  L_0.DimensionUnit_ID,
  L_0.Category_ID,
  L_0.PriceCondition,
  L_0.PriceDetermination
FROM (com_fabboo_materials_Products AS L_0 LEFT JOIN com_fabboo_materials_Products_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_fabboo_materials_Categories AS SELECT
  L_0.ID,
  coalesce(localized_1.Name, L_0.Name) AS Name
FROM (com_fabboo_materials_Categories AS L_0 LEFT JOIN com_fabboo_materials_Categories_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_fabboo_materials_StockAvailability AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description
FROM (com_fabboo_materials_StockAvailability AS L_0 LEFT JOIN com_fabboo_materials_StockAvailability_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_fabboo_materials_Currencies AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description
FROM (com_fabboo_materials_Currencies AS L_0 LEFT JOIN com_fabboo_materials_Currencies_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_fabboo_materials_UnitOfMeasures AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description
FROM (com_fabboo_materials_UnitOfMeasures AS L_0 LEFT JOIN com_fabboo_materials_UnitOfMeasures_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_fabboo_materials_DimensionUnit AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description
FROM (com_fabboo_materials_DimensionUnit AS L_0 LEFT JOIN com_fabboo_materials_DimensionUnit_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_fabboo_sales_Suppliers AS SELECT
  L_0.ID,
  coalesce(localized_1.Name, L_0.Name) AS Name,
  L_0.Adress_Street,
  L_0.Adress_City,
  L_0.Adress_State,
  L_0.Adress_PostalCode,
  L_0.Adress_Country,
  L_0.Email,
  L_0.Phone,
  L_0.Fax
FROM (com_fabboo_sales_Suppliers AS L_0 LEFT JOIN com_fabboo_sales_Suppliers_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_fabboo_sales_Months AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description,
  coalesce(localized_1.ShortDescription, L_0.ShortDescription) AS ShortDescription
FROM (com_fabboo_sales_Months AS L_0 LEFT JOIN com_fabboo_sales_Months_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_com_fabboo_materials_ProductReview AS SELECT
  L.ID,
  L.Name,
  L.Rating,
  L.Comment,
  L.Product_ID
FROM com_fabboo_materials_ProductReview AS L;

CREATE VIEW localized_com_fabboo_sales_OrderItem AS SELECT
  L.ID,
  L.Order_ID,
  L.Product_ID
FROM com_fabboo_sales_OrderItem AS L;

CREATE VIEW localized_com_fabboo_sales_SalesData AS SELECT
  L.ID,
  L.DeliveryDate,
  L.Revenue,
  L.Product_ID,
  L.Currency_ID,
  L.DeliveryMonth_ID
FROM com_fabboo_sales_SalesData AS L;

CREATE VIEW localized_com_fabboo_sales_Order AS SELECT
  L.ID,
  L.Date,
  L.Customer
FROM com_fabboo_sales_Order AS L;

CREATE VIEW Reports_ProductReview AS SELECT
  AverageRating_0.ProductId,
  AverageRating_0.AverageRating
FROM com_fabboo_reports_AverageRating AS AverageRating_0;

CREATE VIEW com_fabboo_reports_Products AS SELECT
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnit_ID,
  Products_0.Category_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination,
  ToAverageRating_1.AverageRating AS Rating,
  CASE WHEN Products_0.Quantity >= 8 THEN 3 WHEN Products_0.Quantity > 0 THEN 2 ELSE 1 END AS StockAvailability
FROM (com_fabboo_materials_Products AS Products_0 LEFT JOIN com_fabboo_reports_AverageRating AS ToAverageRating_1 ON ToAverageRating_1.ProductId = Products_0.ID);

CREATE VIEW localized_ProductService_Supplier AS SELECT
  Suppliers_0.ID,
  Suppliers_0.Name,
  Suppliers_0.Email,
  Suppliers_0.Phone,
  Suppliers_0.Fax
FROM localized_com_fabboo_sales_Suppliers AS Suppliers_0;

CREATE VIEW localized_ProductService_SalesData AS SELECT
  SalesData_0.ID,
  SalesData_0.DeliveryDate,
  SalesData_0.Revenue,
  SalesData_0.Currency_ID AS CurrencyKey,
  DeliveryMonth_1.ID AS DeliveryMonthId,
  DeliveryMonth_1.Description AS DeliveryMonth,
  SalesData_0.Product_ID AS ToProduct_ID
FROM (localized_com_fabboo_sales_SalesData AS SalesData_0 LEFT JOIN localized_com_fabboo_sales_Months AS DeliveryMonth_1 ON SalesData_0.DeliveryMonth_ID = DeliveryMonth_1.ID);

CREATE VIEW localized_ProductService_StockAvailability AS SELECT
  StockAvailability_0.ID,
  StockAvailability_0.Description
FROM localized_com_fabboo_materials_StockAvailability AS StockAvailability_0;

CREATE VIEW localized_ProductService_VH_Categories AS SELECT
  Categories_0.ID AS Code,
  Categories_0.Name AS Text
FROM localized_com_fabboo_materials_Categories AS Categories_0;

CREATE VIEW localized_ProductService_VH_Currencies AS SELECT
  Currencies_0.ID AS Code,
  Currencies_0.Description AS Text
FROM localized_com_fabboo_materials_Currencies AS Currencies_0;

CREATE VIEW localized_ProductService_VH_UnitOfMeasure AS SELECT
  UnitOfMeasures_0.ID AS Code,
  UnitOfMeasures_0.Description AS Text
FROM localized_com_fabboo_materials_UnitOfMeasures AS UnitOfMeasures_0;

CREATE VIEW localized_ProductService_VH_DimenionUnits AS SELECT
  DimensionUnit_0.ID AS Code,
  DimensionUnit_0.Description AS Text
FROM localized_com_fabboo_materials_DimensionUnit AS DimensionUnit_0;

CREATE VIEW localized_MyService_SuppliersProduct AS SELECT
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnit_ID,
  Products_0.Category_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination,
  Supplier_1.Adress_Street,
  Supplier_1.Adress_City,
  Supplier_1.Adress_State,
  Supplier_1.Adress_PostalCode,
  Supplier_1.Adress_Country
FROM (localized_com_fabboo_materials_Products AS Products_0 LEFT JOIN localized_com_fabboo_sales_Suppliers AS Supplier_1 ON Products_0.Supplier_ID = Supplier_1.ID)
WHERE (Supplier_1.Adress_PostalCode = 98074) AND (Products_0.Name = 'Pink Lemonade');

CREATE VIEW localized_MyService_SuppliersToSales AS SELECT
  Supplier_1.Email,
  Category_2.Name,
  Currency_4.ID,
  Currency_4.Description
FROM ((((localized_com_fabboo_materials_Products AS Products_0 LEFT JOIN localized_com_fabboo_sales_Suppliers AS Supplier_1 ON Products_0.Supplier_ID = Supplier_1.ID) LEFT JOIN localized_com_fabboo_materials_Categories AS Category_2 ON Products_0.Category_ID = Category_2.ID) LEFT JOIN localized_com_fabboo_sales_SalesData AS SalesData_3 ON (SalesData_3.Product_ID = Products_0.ID)) LEFT JOIN localized_com_fabboo_materials_Currencies AS Currency_4 ON SalesData_3.Currency_ID = Currency_4.ID);

CREATE VIEW localized_Reports_EntityExists AS SELECT
  Products_0.Name
FROM localized_com_fabboo_materials_Products AS Products_0
